@incollection{williams_users_2017,
	title = {Users’ Views of Ethics in Social Media Research: Informed Consent, Anonymity, and Harm},
	volume = {2},
	isbn = {978-1-78714-486-6 978-1-78714-485-9},
	url = {https://doi.org/10.1108/S2398-601820180000002002},
	series = {Advances in Research Ethics and Integrity},
	shorttitle = {Users’ Views of Ethics in Social Media Research},
	abstract = {Some researchers consider most social media communications as public, and posts from networks such as Twitter are routinely harvested and published without anonymization and without direct consent from users. In this chapter, we argue that researchers must move beyond the permissions granted by ‘legal’ accounts of the use of these new forms of data (e.g., Terms and Conditions) to a more nuanced and reflexive ethical approach that puts user expectations, safety, and privacy rights center stage. Through two projects, we present qualitative and quantitative data that illustrate social media users’ views on the use of their data by researchers. Over four in five report expecting to be asked for their consent and nine in ten expect anonymity ahead of publication of their Twitter posts. Given the unique nature of this online public environment and what we know about users’ views pertaining to informed consent, anonymity, and harm, we conclude researchers seeking to embark on social media research should conduct a risk assessment to determine likely privacy infringement and potential user harm from publishing user content.},
	pages = {27--52},
	booktitle = {The Ethics of Online Research},
	publisher = {Emerald Publishing Limited},
	author = {Williams, Matthew L. and Burnap, Pete and Sloan, Luke and Jessop, Curtis and Lepps, Hayley},
	editor = {Woodfield, Kandy},
	urldate = {2020-12-01},
	date = {2017-01-01},
	doi = {10.1108/S2398-601820180000002002},
	keywords = {Others: ethics - social media research, others},
	file = {Accepted Version:/Users/JiaxiLi/Zotero/storage/BECGI9PL/Williams et al. - 2017 - Users’ Views of Ethics in Social Media Research I.pdf:application/pdf;Snapshot:/Users/JiaxiLi/Zotero/storage/ZWFWDIAW/html.html:text/html}
