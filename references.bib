@article{zimmer_but_2010,
	title = {“But the data is already public”: on the ethics of research in Facebook},
	volume = {12},
	issn = {1388-1957, 1572-8439},
	url = {http://link.springer.com/10.1007/s10676-010-9227-5},
	doi = {10.1007/s10676-010-9227-5},
	shorttitle = {“But the data is already public”},
	abstract = {In 2008, a group of researchers publicly released proﬁle data collected from the Facebook accounts of an entire cohort of college students from a {US} university. While good-faith attempts were made to hide the identity of the institution and protect the privacy of the data subjects, the source of the data was quickly identiﬁed, placing the privacy of the students at risk. Using this incident as a case study, this paper articulates a set of ethical concerns that must be addressed before embarking on future research in social networking sites, including the nature of consent, properly identifying and respecting expectations of privacy on social network sites, strategies for data anonymization prior to public release, and the relative expertise of institutional review boards when confronted with research projects based on data gleaned from social media.},
	pages = {313--325},
	number = {4},
	journaltitle = {Ethics and Information Technology},
	shortjournal = {Ethics Inf Technol},
	author = {Zimmer, Michael},
	urldate = {2020-11-24},
	date = {2010-12},
	langid = {english},
	keywords = {Others: ethics - social media research, most cited}
}

